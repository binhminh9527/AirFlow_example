{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug PolygonValidator (invalid_polygons.shp)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/dags/bin/PolygonValidator_bin",
      "args": ["${workspaceFolder}/Shapefile_validity_test/invalid_polygons.shp"],
      "cwd": "${workspaceFolder}",
      "environment": [],
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        },
        {
          "description": "Skip all automatic library loading",
          "text": "-gdb-set auto-solib-add off",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build-polygon-validator",
      "miDebuggerPath": "/usr/bin/gdb",
      "externalConsole": false,
      "stopAtEntry": true,
      "sourceFileMap": {}
    }
  ]
}
